{
  "version": 2,
  "builds": [
    {
      "src": "Backend/server.js",  // Correct path to server.js in the Backend folder
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Backend/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "Backend/server.js"
    }
  ]
}
